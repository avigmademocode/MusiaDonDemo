<!DOCTYPE html>

<html>
<head>
    <meta name="viewport" content="width=device-width" />

    <meta charset="utf-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <meta name="description" content="">
    <meta name="author" content="Mosaddek">
    <meta name="keyword" content="FlatLab, Dashboard, Bootstrap, Admin, Template, Theme, Responsive, Fluid, Retina">
    <title>Musiadon</title>
    <link rel="shortcut icon" href="img/favicon.html">
    <!-- Kendo CSS -->

    <link href="~/kendocss/kendo.common.min.css" rel="stylesheet" />
    <link href="~/kendocss/kendo.metro.min.css" rel="stylesheet" />

    <!-- Bootstrap core CSS -->

    <link href="~/PScript/css/bootstrap.min.css" rel="stylesheet">
    <link href="~/PScript/css/bootstrap-reset.css" rel="stylesheet">
    <!--external css-->
    <link href="~/PScript/assets/font-awesome/css/font-awesome.css" rel="stylesheet" />
    <link href="~/PScript/assets/jquery-easy-pie-chart/jquery.easy-pie-chart.css" rel="stylesheet" type="text/css" media="screen" />
    <link rel="stylesheet" href="~/PScript/css/owl.carousel.css" type="text/css">
    <link href="~/kendocss/jquery.dataTables.min.css" rel="stylesheet" />
    <!--right slidebar-->
    <link href="~/PScript/css/slidebars.css" rel="stylesheet">

    <!-- Custom styles for this template -->

    <link href="~/PScript/css/style.css" rel="stylesheet">
    <link href="~/PScript/css/style-responsive.css" rel="stylesheet" />

    <script src="~/AngularJs/angular.min.js"></script>
    <script src="~/AngularJs/angucomplete-alt.min.js"></script>
    <script src="~/AngularJs/HomeApp.js"></script>

    <script src="~/Scripts/jquery-3.3.1.min.js"></script>
    <style>
        .content{
            display:none;
        }
        .preload{
            margin:0px;
            position:absolute;
            top:50%;
            left:60%;
            margin-right:-50%;
            transform:translate(-50%, -50%);
        }
    </style>


    <!--[if lt IE 9]>
      <script src="js/html5shiv.js"></script>
      <script src="js/respond.min.js"></script>
    <![endif]-->
</head>
<body ng-app="HomeApp">
    <section id="container">
        <!--header start-->
        @Html.Partial("_TopMenuLayout")
        <!--header end-->
        <!--sidebar start-->
        @Html.Partial("_LeftMenuLayout")
        <!--sidebar end-->

        <section id="main-content">
            <section class="wrapper">
                <div class="preload">
                    @*<img src="~/Images/loading.gif" />*@
                    <img src="~/Images/loader.gif" width="70px;" height="70px" />
                </div>
                <div class="content">

                    @RenderBody()
                    <!--state overview start-->
                    <!--state overview end-->

                </div>
            </section>
        </section>

        <!-- Right Slidebar start -->
        @Html.Partial("_RightMenuLayout")
        <!-- Right Slidebar end -->
        <!--footer start-->
        <footer class="site-footer">
            <div class="text-center">
                @*2013 &copy; FlatLab by VectorLab.*@
                <a href="#" class="go-top">
                    <i class="fa fa-angle-up"></i>
                </a>
            </div>
        </footer>
        <!--footer end-->
    </section>
    <!-- js placed at the end of the document so the pages load faster -->
    <script src="~/PScript/js/jquery.js"></script>
    <script src="~/PScript/js/bootstrap.min.js"></script>
    <script class="include" type="text/javascript" src="~/PScript/js/jquery.dcjqaccordion.2.7.js"></script>
    <script src="~/PScript/js/jquery.scrollTo.min.js"></script>
    <script src="~/PScript/js/jquery.nicescroll.js" type="text/javascript"></script>
    <script src="~/PScript/js/jquery.sparkline.js" type="text/javascript"></script>
    <script src="~/PScript/assets/jquery-easy-pie-chart/jquery.easy-pie-chart.js"></script>
    <script src="~/PScript/js/owl.carousel.js"></script>
    <script src="~/PScript/js/jquery.customSelect.min.js"></script>
    <script src="~/PScript/js/respond.min.js"></script>
    <script src="~/Scripts/jquery.dataTables.min.js"></script>
    <!--right slidebar-->
    <script src="~/PScript/js/slidebars.min.js"></script>

    <!--common script for all pages-->
    <script src="~/PScript/js/common-scripts.js"></script>

    <!--script for this page-->
    <script src="~/PScript/js/sparkline-chart.js"></script>
    <script src="~/PScript/js/easy-pie-chart.js"></script>
    <script src="~/PScript/js/count.js"></script>

    <script>

        //owl carousel

        $(document).ready(function () {
            $("#owl-demo").owlCarousel({
                navigation: true,
                slideSpeed: 300,
                paginationSpeed: 400,
                singleItem: true,
                autoPlay: true

            });
        });

        //custom select box

        $(function () {
            $('select.styled').customSelect();
        });

        $(window).on("resize", function () {
            var owl = $("#owl-demo").data("owlCarousel");
            owl.reinit();
        });


    </script>
    <script>

        $(function () {
            $(".preload").fadeOut(2000, function () {
                $(".content").fadeIn(1000);
            });
        });
    </script>

    <script src="~/Scripts/kendo/2012.3.1114/kendo.web.min.js"></script>
</body>
</html>
